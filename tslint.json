{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "arrow-parens": false,
        "interface-name": false,
        "no-console": false,
        "no-consecutive-blank-lines": false,
        "object-literal-sort-keys": false,
        "quotemark": [true, "single", "avoid-escape"],
        "semicolon": [true, "always", "ignore-interfaces"],
        "trailing-comma": false
    },
    "rulesDirectory": []
}